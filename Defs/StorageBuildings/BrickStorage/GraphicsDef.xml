<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Block Pile 2x1 -->
  
	<AdaptiveStorage.GraphicsDef>
		<defName>ASMedievalBlockPileA</defName>
		<targetDef>ASMedievalBlockPileA</targetDef>
		<graphics>
			<li>
				<graphicData>
					<texPath>ASMedieval/BlockPile/BlockPile2x1_A</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(3.0,2.0)</drawSize>
				</graphicData>
			</li>
			<li>
				<graphicData>
					<texPath>ASMedieval/BlockPile/BlockPile2x1_B</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(3.0,2.0)</drawSize>
				</graphicData>
				<minimumStackCount>1</minimumStackCount>
			</li>
			<li>
				<graphicData>
					<texPath>ASMedieval/BlockPile/BlockPile2x1_C</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(3.0,2.0)</drawSize>
				</graphicData>
				<minimumStackCount>4</minimumStackCount>
			</li>
		</graphics>
		<showContainedItems>false</showContainedItems>
		<useDominantContentColor>true</useDominantContentColor>
	</AdaptiveStorage.GraphicsDef>

